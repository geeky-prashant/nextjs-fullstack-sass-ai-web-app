generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model UserApiLimit {
  id String @id @default(cuid()) @map("_id") @db.ObjectId
  userId String @unique @db.ObjectId
  count Int @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
